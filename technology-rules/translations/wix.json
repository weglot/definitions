{
  "$schema": "../schemas/translations.schema.json",
  "title": "Wix",
  "examples": ["https://fr.weglot-translate-wix.com/"],
  "translations": [
    {
      "type": "JSON",
      "condition": [
        {
          "type": "TECHNOLOGY_ID",
          "payload": 6
        },
        {
          "type": "URI_MATCH",
          "payload": {
            "type": "START_WITH",
            "value": "https://siteassets.parastorage.com/pages/singlePage"
          }
        },
        {
          "type": "URI_MATCH",
          "payload": {
            "type": "START_WITH",
            "value": "https://siteassets.parastorage.com/pages/fixedPage"
          }
        },
        {
          "type": "URI_MATCH",
          "payload": {
            "type": "START_WITH",
            "value": "https://siteassets.parastorage.com/pages/pages/viewerViewModeJson"
          }
        },
        {
          "type": "URI_MATCH",
          "payload": {
            "type": "START_WITH",
            "value": "https://siteassets.parastorage.com/singlePage"
          }
        },
        {
          "type": "URI_MATCH",
          "payload": {
            "type": "START_WITH",
            "value": "https://siteassets.parastorage.com/pages/pages/thunderbolt"
          }
        },
        {
          "type": "URI_MATCH",
          "payload": {
            "type": "MATCH_REGEX",
            "value": "https://pages.wixstatic.com/sites/([\\w_]+).json.z"
          }
        }
      ],
      "value": [
        {
          "paths": [
            "$..text",
            "$..label",
            "$..title",
            "$..descriptionSEO",
            "$..logOutText",
            "$..loggedInText",
            "$..loggedOutText",
            "$..pageTitleSEO",
            "$.tags..children",
            "$..pageTopLabel",
            "$..pageName",
            "$..menuLabel",
            "$.props.render..html"
          ],
          "wordType": 1
        },
        {
          "paths": ["$..advancedSeoData"],
          "type": "JSON"
        }
      ]
    },
    {
      "type": "JSON",
      "condition": [
        {
          "type": "TECHNOLOGY_ID",
          "payload": 6
        },
        {
          "type": "URI_MATCH",
          "payload": {
            "type": "START_WITH",
            "value": "https://wixlabs-wix-faq-11.appspot.com/api/"
          }
        }
      ],
      "value": [
        {
          "paths": [
            "$.FAQItems.questions[*].title.text",
            "$.FAQItems.questions[*].text.text",
            "$.settings.title.text",
            "$.settings.searchBar.text"
          ],
          "wordType": 1
        }
      ]
    },
    {
      "type": "JSON",
      "condition": [
        {
          "type": "TECHNOLOGY_ID",
          "payload": 6
        },
        {
          "type": "PATH_MATCH",
          "payload": {
            "type": "START_WITH",
            "value": "/_api/wix-ecommerce-storefront-web/api"
          }
        }
      ],
      "value": [
        {
          "paths": [
            "$.data.catalog..name",
            "$.data.catalog..description",
            "$.data.catalog..title",
            "$.data.catalog..tagline",
            "$.data.catalog..seoDescription"
          ],
          "wordType": 1
        }
      ]
    },
    {
      "type": "JSON",
      "condition": [
        {
          "type": "TECHNOLOGY_ID",
          "payload": 6
        },
        {
          "type": "PATH_MATCH",
          "payload": {
            "type": "START_WITH",
            "value": "/_serverless/comments-web-translations-proxy/comments-ooi/"
          }
        },
        {
          "type": "PATH_MATCH",
          "payload": {
            "type": "START_WITH",
            "value": "/services/communities-blog-translations/"
          }
        }
      ],
      "value": [
        {
          "paths": ["$[*]"],
          "wordType": 1
        }
      ]
    },
    {
      "type": "JSON",
      "condition": [
        {
          "type": "TECHNOLOGY_ID",
          "payload": 6
        },
        {
          "type": "PATH_MATCH",
          "payload": {
            "type": "IS_EXACTLY",
            "value": "/_api/communities-blog-node-api/_api/posts"
          }
        }
      ],
      "value": [
        {
          "paths": ["$[*].title", "$[*].excerpt", "$.content.blocks[*].text"],
          "wordType": 1
        }
      ]
    },
    {
      "type": "JSON",
      "condition": [
        {
          "type": "TECHNOLOGY_ID",
          "payload": 6
        },
        {
          "type": "PATH_MATCH",
          "payload": {
            "type": "START_WITH",
            "value": "/_api/communities-blog-node-api/_api/blogs/this"
          }
        }
      ],
      "value": [
        {
          "paths": ["$.menuLabel"],
          "wordType": 1
        }
      ]
    }
  ]
}
