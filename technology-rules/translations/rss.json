{
  "$schema": "../schemas/translations.schema.json",
  "title": "SVG",
  "translations": [
    {
      "type": "XML",
      "condition": [
        {
          "type": "XML_ATTRIBUTE_VALUE",
          "payload": {
            "selector": "rss",
            "attr": "xmlns:media",
            "value": "http://search.yahoo.com/mrss/"
          }
        }
      ],
      "entryContext": "item",
      "value": [
        {
          "paths": ["title", "description"],
          "wordType": 1
        },
        {
          "paths": ["media:content", "media:thumbnail"],
          "wordType": 6
        }
      ]
    }
  ]
}
