{
  "$schema": "../../schemas/proxy.schema.json",
  "title": "Wordpress proxy rules",
  "technology_id": 1,
  "proxy": [
    {
      "condition": [
        {
          "type": "HOST_MATCH",
          "payload": {
            "type": "END_WITH",
            "value": ".algolia.net"
          }
        }
      ],
      "value": [
        {
          "action": "ALLOW_HOST"
        }
      ]
    }
  ]
}
